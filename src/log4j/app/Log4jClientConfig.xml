<?xml version="1.0" encoding="UTF-8" ?>

<client>
	<name>log4jwebapp</name>
	<description>The Log4j configuration web application</description>
	<service>false</service>
	<allowAnonymous>false</allowAnonymous>
	<session-timeout>900000</session-timeout>
	<monitors>
	</monitors>
	<plugins>
		<plugin>
			<name>UI</name>
			<class>prisms.ui.UI</class>
		</plugin>
		<plugin>
			<name>Logger Tree</name>
			<class>log4j.ui.LoggerTree</class>
		</plugin>
		<plugin>
			<name>Logger Editor</name>
			<class>log4j.ui.LoggerEditor</class>
		</plugin>
	</plugins>
</client>