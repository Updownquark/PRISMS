<?xml version="1.0" encoding="UTF-8" ?>

<application>
	<version>1.3.0</version>
	<modified>13May2010</modified>
	<worker>
		<type>threadpool</type>
	</worker>
	<properties>
		<property type="prisms.util.persisters.CommonValueManager">
			<field>manager.app.ManagerProperties.applications</field>
			<persister>
				<class>manager.persisters.AppPersister</class>
			</persister>
			<changeEvent>
				<name>appChanged</name>
				<eventProperty>app</eventProperty>
			</changeEvent>
		</property>
		<property type="prisms.util.persisters.CommonValueManager">
			<field>manager.app.ManagerProperties.users</field>
			<persister>
				<class>manager.persisters.UserPersister</class>
			</persister>
			<changeEvent>
				<name>userChanged</name>
				<eventProperty>user</eventProperty>
			</changeEvent>
		</property>
	</properties>
	<events>
		<event>
			<name>userChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>userPermissionsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>groupChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>appChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>permissionChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>appGroupsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>appPermissionsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>groupPermissionsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>userGroupsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>clientChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
		<event>
			<name>appClientsChanged</name>
			<class>prisms.util.EventGlobalizer</class>
		</event>
	</events>
	<monitors>
		<monitor>
			<class>manager.app.ManagerMonitor</class>
		</monitor>
	</monitors>
</application>
