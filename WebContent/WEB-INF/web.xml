<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">

<web-app>

	<listener>
		<listener-class>org.apache.commons.fileupload.servlet.FileCleanerCleanup</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>PRISMSWebServlet</servlet-name>
		<servlet-class>prisms.arch.PrismsServer</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>PRISMSWebServlet</servlet-name>
		<url-pattern>/prisms</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>PRISMSWebServlet</servlet-name>
		<url-pattern>/prismsImage.png</url-pattern>
	</servlet-mapping>

</web-app>
